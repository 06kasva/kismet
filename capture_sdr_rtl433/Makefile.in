include ../Makefile.inc

MONITOR_OBJ = 
MONITOR_BIN = kismet_cap_sdr_rtl433

all: $(MONITOR_BIN)

$(MONITOR_BIN):	$(MONITOR_OBJ) 
	$(PYTHON2) ./setup.py build

install:
	$(PYTHON2) ./setup.py install
	$(INSTALL) -o $(INSTUSR) -g $(INSTGRP) $(MONITOR_BIN) $(BIN)/$(MONITOR_BIN)

clean:
	@-$(PYTHON2) ./setup.py clean

